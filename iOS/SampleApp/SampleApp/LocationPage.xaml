    x:Class="SampleApp.LocationPage"
    xmlns:ios="clr-namespace:Xamarin.Forms.PlatformConfiguration.iOSSpecific;assembly=Xamarin.Forms.Core"
    ios:Page.UseSafeArea="true"
    Title="LocationSample">
    <ContentPage.Content>
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition></RowDefinition>
                <RowDefinition></RowDefinition>
            </Grid.RowDefinitions>
            <Label Grid.Row="0" x:Name="AddressLabel"></Label>
            <Label Grid.Row="1" x:Name="AddressDataLabel"></Label>
        </Grid>
    </ContentPage.Content>
</ContentPage>
